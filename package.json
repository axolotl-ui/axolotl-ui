    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-unused-imports": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "terser": "latest",
    "tsup": "latest",
    "typescript": "latest"
  }
